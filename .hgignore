syntax: glob
*.log*

#Eclipse project files are not to be version tracked:
.classpath
.project
.settings

#Eclipse workspace are not to be version tracked:
#works for windows:
workspace\
#Works for Linux
workspace/*

#IntelliJ project files are not to be version tracked:
*.iml
*.ipr
*.iws

#Maven build products are not to be version tracked:
#In windows:
target\
#In Linux
*/target/*

#Gradle build products are not to be version tracked:
#In windows:
.gradle\
build\
bin\
#In Linux
*/build/*
*/bin/*
*/.gradle/*


syntax: regexp
